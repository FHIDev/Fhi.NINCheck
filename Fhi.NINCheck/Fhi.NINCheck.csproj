<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.5.0</Version>
    <RepositoryUrl>https://github.com/folkehelseinstituttet/Fhi.NINCheck</RepositoryUrl>
    <PackageProjectUrl>https://github.com/folkehelseinstituttet/Fhi.NINCheck</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIconUrl>fhi.png</PackageIconUrl>
    <authors>Folkehelseinstituttet (FHI)</authors>
    <Copyright>(c) 2022 - 2024, Public Health Institute (FHI), Norway</Copyright>
    <PackageIcon>fhi.png</PackageIcon>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="\" />
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="images\fhi.png" Pack="True" PackagePath="\" />    
  </ItemGroup>
</Project>
